{
  "name": "server",
  "version": "0.1.0",
  "description": "backend",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "start:debug": "cross-env DEBUG=server:* npm run start",
    "dev": "nodemon index.js",
    "dev:debug": "cross-env DEBUG=server:* npm run dev",
    "format": "prettier --write **/*.js",
    "lint": "eslint  --ext .js server config",
    "lint:watch": "npm run lint -- --watch",
    "precommit": "npm run lint && npm run test",
    "test": "cross-env NODE_ENV=test ./node_modules/.bin/mocha --ui bdd --reporter spec --colors server --recursive",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "cross-env NODE_ENV=test ./node_modules/.bin/istanbul cover _mocha -- --ui bdd --reporter spec --colors server --recursive",
    "test:check-coverage": "npm run test:coverage && istanbul check-coverage",
    "report:coverage": "codecov"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:kunaltawatia/mern.git"
  },
  "dependencies": {
    "bluebird": "3.7.2",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "4.17.1",
    "express-jwt": "6.0.0",
    "express-session": "^1.17.1",
    "express-validation": "3.0.5",
    "express-winston": "4.0.3",
    "google-auth-library": "^6.0.0",
    "helmet": "3.22.0",
    "http-status": "1.4.2",
    "jsonwebtoken": "8.5.1",
    "method-override": "^3.0.0",
    "mongoose": "5.9.16",
    "morgan": "1.10.0",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "chai": "4.2.0",
    "codecov": "^3.7.0",
    "commitizen": "^4.1.2",
    "cross-env": "7.0.2",
    "cz-conventional-changelog": "3.2.0",
    "eslint": "^7.1.0",
    "eslint-config-airbnb-base": "14.1.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-watch": "6.0.1",
    "husky": "4.2.5",
    "istanbul": "1.1.0-alpha.1",
    "mocha": "7.2.0",
    "prettier": "^2.0.5",
    "supertest": "4.0.2",
    "supertest-as-promised": "4.0.2",
    "validate-commit-msg": "^2.14.0"
  },
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
